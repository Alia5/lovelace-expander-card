{
    "name": "lovelace-expander-card",
    "version": "0.0.1",
    "type": "module",
    "watch": {
        "buildAndStart": {
            "patterns": [
                "src"
            ],
            "extensions": "svelte"
        }
    },
    "scripts": {
        "build": "VERSION=$(git describe --tags) vite build",
